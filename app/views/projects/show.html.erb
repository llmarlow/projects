<div class="booyah-box col-xs-10 col-xs-offset-1">
	<h1><%= @project.name %></h1>
	<p><%= @project.description %></p>

<div class="clear"></div>
<%= simple_form_for @product, :url => project_product_path(@project) do |f| %>
    <%= f.input :name %>
    <%= f.input :quantity %>
    <br />
    <%= f.submit 'Create', :class => 'btn btn-primary' %>
<% end %>
</div>
<%= params.inspect %>